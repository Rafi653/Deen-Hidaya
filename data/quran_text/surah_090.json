  "surah_info": {
    "id": 90,
    "revelation_place": "makkah",
    "revelation_order": 35,
    "bismillah_pre": true,
    "name_simple": "Al-Balad",
    "name_complex": "Al-Balad",
    "name_arabic": "البلد",
    "verses_count": 20,
    "pages": [
      594,
