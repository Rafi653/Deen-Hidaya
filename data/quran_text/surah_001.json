  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "verse_key": "1:1",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "text_imlaei": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96343,
          "resource_id": 20,
          "text": "In the name of Allāh,<sup foot_note=195932>1</sup> the Entirely Merciful, the Especially Merciful.<sup foot_note=195931>2</sup>"
        },
        {
          "id": 953850,
          "resource_id": 140,
          "text": "(Empiezo)<sup foot_note=149054>1</sup> con el nombre de Al-lah<sup foot_note=149055>2</sup>, el Clemente, el Misericordioso<sup foot_note=149056>3</sup>."
        }
      ]
    },
    {
      "id": 2,
      "verse_number": 2,
      "verse_key": "1:2",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "ٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَـٰلَمِينَ",
      "text_imlaei": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96493,
          "resource_id": 20,
          "text": "[All] praise is [due] to Allāh, Lord<sup foot_note=195933>1</sup> of the worlds -"
        },
        {
          "id": 953851,
          "resource_id": 140,
          "text": "¡Alabado sea Al-lah, Señor de toda la creación!,"
        }
      ]
    },
    {
      "id": 3,
      "verse_number": 3,
      "verse_key": "1:3",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "text_imlaei": "الرَّحْمَٰنِ الرَّحِيمِ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96606,
          "resource_id": 20,
          "text": "The Entirely Merciful, the Especially Merciful,"
        },
        {
          "id": 953852,
          "resource_id": 140,
          "text": "el Clemente, el Misericordioso,"
        }
      ]
    },
    {
      "id": 4,
      "verse_number": 4,
      "verse_key": "1:4",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "مَـٰلِكِ يَوْمِ ٱلدِّينِ",
      "text_imlaei": "مَالِكِ يَوْمِ الدِّينِ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96653,
          "resource_id": 20,
          "text": "Sovereign of the Day of Recompense.<sup foot_note=195934>1</sup>"
        },
        {
          "id": 953853,
          "resource_id": 140,
          "text": "Soberano del Día del Juicio Final<sup foot_note=149057>1</sup>."
        }
      ]
    },
    {
      "id": 5,
      "verse_number": 5,
      "verse_key": "1:5",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
      "text_imlaei": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96707,
          "resource_id": 20,
          "text": "It is You we worship and You we ask for help."
        },
        {
          "id": 953854,
          "resource_id": 140,
          "text": "Solo a Ti adoramos y solo a Ti pedimos ayuda."
        }
      ]
    },
    {
      "id": 6,
      "verse_number": 6,
      "verse_key": "1:6",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "ٱهْدِنَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
      "text_imlaei": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96503,
          "resource_id": 20,
          "text": "Guide us to the straight path -"
        },
        {
          "id": 953855,
          "resource_id": 140,
          "text": "Guíanos por el camino recto,"
        }
      ]
    },
    {
      "id": 7,
      "verse_number": 7,
      "verse_key": "1:7",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "صِرَٰطَ ٱلَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ ٱلْمَغْضُوبِ عَلَيْهِمْ وَلَا ٱلضَّآلِّينَ",
      "text_imlaei": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 96932,
          "resource_id": 20,
          "text": "The path of those upon whom You have bestowed favor, not of those who have earned [Your] anger or of those who are astray."
        },
        {
          "id": 953856,
          "resource_id": 140,
          "text": "el camino de quienes has agraciado, no el de quienes han incurrido en Tu ira ni el de quienes se han extraviado (a pesar de conocer la verdad)."
        }
      ]
    }
  ],
