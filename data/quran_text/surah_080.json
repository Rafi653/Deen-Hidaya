  "surah_info": {
    "id": 80,
    "revelation_place": "makkah",
    "revelation_order": 24,
    "bismillah_pre": true,
    "name_simple": "'Abasa",
    "name_complex": "`Abasa",
    "name_arabic": "عبس",
    "verses_count": 42,
    "pages": [
      585,
      585
    ],
    "translated_name": {
      "language_name": "english",
      "name": "He Frowned"
    }
  },
  "verses": [
    {
      "id": 5759,
      "verse_number": 1,
      "verse_key": "80:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " عَبَسَ وَتَوَلَّىٰٓ",
      "text_imlaei": "عَبَسَ وَتَوَلَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101803,
          "resource_id": 20,
          "text": "He [i.e., the Prophet (ﷺ) ] frowned and turned away"
        },
        {
          "id": 1172355,
          "resource_id": 213,
          "text": "তিনি ভ্ৰকুঞ্চিত করলেন এবং মুখ ফিরিয়ে নিলেন [১] ,"
        }
      ]
    },
    {
      "id": 5760,
      "verse_number": 2,
      "verse_key": "80:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
      "text_imlaei": "أَن جَاءَهُ الْأَعْمَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101814,
          "resource_id": 20,
          "text": "Because there came to him the blind man,<sup foot_note=197703>1</sup> [interrupting]."
        },
        {
          "id": 1172356,
          "resource_id": 213,
          "text": "কারণ তাঁর কাছে অন্ধ লোকটি আসল।"
        }
      ]
    },
    {
      "id": 5761,
      "verse_number": 3,
      "verse_key": "80:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "text_imlaei": "وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101825,
          "resource_id": 20,
          "text": "But what would make you perceive, [O Muḥammad], that perhaps he might be purified<sup foot_note=197704>1</sup>"
        },
        {
          "id": 1172357,
          "resource_id": 213,
          "text": "আর কিসে আপনাকে জানাবে যে, --- সে হয়ত পরিশুদ্ধ হত"
        }
      ]
    },
    {
      "id": 5762,
      "verse_number": 4,
      "verse_key": "80:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
      "text_imlaei": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ الذِّكْرَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101836,
          "resource_id": 20,
          "text": "Or be reminded and the remembrance would benefit him?"
        },
        {
          "id": 1172358,
          "resource_id": 213,
          "text": "অথবা উপদেশ গ্ৰহণ করত, ফলে সে উপদেশ তার উপকারে আসত [১]।"
        }
      ]
    },
    {
      "id": 5763,
      "verse_number": 5,
      "verse_key": "80:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
      "text_imlaei": "أَمَّا مَنِ اسْتَغْنَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101840,
          "resource_id": 20,
          "text": "As for he who thinks himself without need,<sup foot_note=197705>1</sup>"
        },
        {
          "id": 1172359,
          "resource_id": 213,
          "text": "আর যে পরোয়া করে না"
        }
      ]
    },
    {
      "id": 5764,
      "verse_number": 6,
      "verse_key": "80:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "text_imlaei": "فَأَنتَ لَهُ تَصَدَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101841,
          "resource_id": 20,
          "text": "To him you give attention."
        },
        {
          "id": 1172360,
          "resource_id": 213,
          "text": "আপনি তার প্রতি মনোযোগ দিয়েছেন।"
        }
      ]
    },
    {
      "id": 5765,
      "verse_number": 7,
      "verse_key": "80:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "text_imlaei": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101842,
          "resource_id": 20,
          "text": "And not upon you [is any blame] if he will not be purified.<sup foot_note=197706>1</sup>"
        },
        {
          "id": 1172361,
          "resource_id": 213,
          "text": "অথচ সে নিজে পরিশুদ্ধ না হলে আপনার কোনো দায়িত্ব নেই,"
        }
      ]
    },
    {
      "id": 5766,
      "verse_number": 8,
      "verse_key": "80:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
      "text_imlaei": "وَأَمَّا مَن جَاءَكَ يَسْعَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101843,
          "resource_id": 20,
          "text": "But as for he who came to you striving [for knowledge]"
        },
        {
          "id": 1172362,
          "resource_id": 213,
          "text": "অপরদিকে যে আপনার কাছে ছুটে এলো,"
        }
      ]
    },
    {
      "id": 5767,
      "verse_number": 9,
      "verse_key": "80:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ يَخْشَىٰ",
      "text_imlaei": "وَهُوَ يَخْشَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101844,
          "resource_id": 20,
          "text": "While he fears [Allāh],"
        },
        {
          "id": 1172363,
          "resource_id": 213,
          "text": "আর সে সশঙ্কচিত্ত,"
        }
      ]
    },
    {
      "id": 5768,
      "verse_number": 10,
      "verse_key": "80:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
      "text_imlaei": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101804,
          "resource_id": 20,
          "text": "From him you are distracted."
        },
        {
          "id": 1172364,
          "resource_id": 213,
          "text": "আপনি তার থেকে উদাসীন হলেন;"
        }
      ]
    },
    {
      "id": 5769,
      "verse_number": 11,
      "verse_key": "80:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّهَا تَذْكِرَةٌ",
      "text_imlaei": "كَلَّا إِنَّهَا تَذْكِرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101805,
          "resource_id": 20,
          "text": "No! Indeed, they [i.e., these verses] are a reminder;"
        },
        {
          "id": 1172365,
          "resource_id": 213,
          "text": "কখনো নয়, এটা তো উপদেশ বাণী [১],"
        }
      ]
    },
    {
      "id": 5770,
      "verse_number": 12,
      "verse_key": "80:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَن شَآءَ ذَكَرَهُۥ",
      "text_imlaei": "فَمَن شَاءَ ذَكَرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101806,
          "resource_id": 20,
          "text": "So whoever wills may remember it.<sup foot_note=197707>1</sup>"
        },
        {
          "id": 1172366,
          "resource_id": 213,
          "text": "কাজেই যে ইচ্ছে করবে সে এটা স্মরণ রাখবে,"
        }
      ]
    },
    {
      "id": 5771,
      "verse_number": 13,
      "verse_key": "80:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى صُحُفٍ مُّكَرَّمَةٍ",
      "text_imlaei": "فِي صُحُفٍ مُّكَرَّمَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101807,
          "resource_id": 20,
          "text": "[It is recorded] in honored sheets,"
        },
        {
          "id": 1172367,
          "resource_id": 213,
          "text": "এটা আছে মর্যাদা সম্পন্ন লিপিসমূহে"
        }
      ]
    },
    {
      "id": 5772,
      "verse_number": 14,
      "verse_key": "80:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ",
      "text_imlaei": "مَّرْفُوعَةٍ مُّطَهَّرَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101808,
          "resource_id": 20,
          "text": "Exalted and purified,"
        },
        {
          "id": 1172368,
          "resource_id": 213,
          "text": "যা উন্নত, পবিত্ৰ [১],"
        }
      ]
    },
    {
      "id": 5773,
      "verse_number": 15,
      "verse_key": "80:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بِأَيْدِى سَفَرَةٍ",
      "text_imlaei": "بِأَيْدِي سَفَرَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101809,
          "resource_id": 20,
          "text": "[Carried] by the hands of messenger-angels,"
        },
        {
          "id": 1172369,
          "resource_id": 213,
          "text": "লেখক বা দূতদের হাতে [১]।"
        }
      ]
    },
    {
      "id": 5774,
      "verse_number": 16,
      "verse_key": "80:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كِرَامٍۭ بَرَرَةٍ",
      "text_imlaei": "كِرَامٍ بَرَرَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101810,
          "resource_id": 20,
          "text": "Noble and dutiful."
        },
        {
          "id": 1172370,
          "resource_id": 213,
          "text": "(যারা) মহাসম্মানিত ও নেককার।"
        }
      ]
    },
    {
      "id": 5775,
      "verse_number": 17,
      "verse_key": "80:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُتِلَ ٱلْإِنسَـٰنُ مَآ أَكْفَرَهُۥ",
      "text_imlaei": "قُتِلَ الْإِنسَانُ مَا أَكْفَرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101811,
          "resource_id": 20,
          "text": "Destroyed [i.e., cursed] is man;<sup foot_note=197708>1</sup> how disbelieving is he."
        },
        {
          "id": 1172371,
          "resource_id": 213,
          "text": "মানুষ ধ্বংস হোক! সে কত অকৃতজ্ঞ [১]!"
        }
      ]
    },
    {
      "id": 5776,
      "verse_number": 18,
      "verse_key": "80:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
      "text_imlaei": "مِنْ أَيِّ شَيْءٍ خَلَقَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101812,
          "resource_id": 20,
          "text": "From what thing [i.e., substance] did He create him?"
        },
        {
          "id": 1172372,
          "resource_id": 213,
          "text": "তিনি তাকে কোন বস্তু থেকে সৃষ্টি করেছেন?"
        }
      ]
    },
    {
      "id": 5777,
      "verse_number": 19,
      "verse_key": "80:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "text_imlaei": "مِن نُّطْفَةٍ خَلَقَهُ فَقَدَّرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101813,
          "resource_id": 20,
          "text": "From a sperm-drop He created him and destined for him;<sup foot_note=197709>1</sup>"
        },
        {
          "id": 1172373,
          "resource_id": 213,
          "text": "শুক্রবিন্দু থেকে, তিনি তাকে সৃষ্টি করেন, পরে তার পরিমিত বিকাশ সাধন করেন [১],"
        }
      ]
    },
    {
      "id": 5778,
      "verse_number": 20,
      "verse_key": "80:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "text_imlaei": "ثُمَّ السَّبِيلَ يَسَّرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101815,
          "resource_id": 20,
          "text": "Then He eased the way for him;<sup foot_note=197710>1</sup>"
        },
        {
          "id": 1172374,
          "resource_id": 213,
          "text": "তারপর তার জন্য পথ সহজ করে দেন [১];"
        }
      ]
    },
    {
      "id": 5779,
      "verse_number": 21,
      "verse_key": "80:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
      "text_imlaei": "ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101816,
          "resource_id": 20,
          "text": "Then He causes his death and provides a grave for him.<sup foot_note=197711>1</sup>"
        },
        {
          "id": 1172375,
          "resource_id": 213,
          "text": "এরপর তার মৃত্যু ঘটান এবং তাকে কবরস্থ করেন।"
        }
      ]
    },
    {
      "id": 5780,
      "verse_number": 22,
      "verse_key": "80:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "text_imlaei": "ثُمَّ إِذَا شَاءَ أَنشَرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101817,
          "resource_id": 20,
          "text": "Then when He wills, He will resurrect him."
        },
        {
          "id": 1172376,
          "resource_id": 213,
          "text": "এরপর যখন ইচ্ছে তিনি তাকে পুনর্জীবিত করবেন [১]।"
        }
      ]
    },
    {
      "id": 5781,
      "verse_number": 23,
      "verse_key": "80:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
      "text_imlaei": "كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101818,
          "resource_id": 20,
          "text": "No! He [i.e., man] has not yet accomplished what He commanded him."
        },
        {
          "id": 1172377,
          "resource_id": 213,
          "text": "কখনো নয়, তিনি তাকে যা আদেশ করেছেন, সে এখনো তা পূর্ণ করেনি।"
        }
      ]
    },
    {
      "id": 5782,
      "verse_number": 24,
      "verse_key": "80:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "text_imlaei": "فَلْيَنظُرِ الْإِنسَانُ إِلَىٰ طَعَامِهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101819,
          "resource_id": 20,
          "text": "Then let mankind look at his food -"
        },
        {
          "id": 1172378,
          "resource_id": 213,
          "text": "অতঃপর মানুষ যেন তার খাদ্যের প্রতি লক্ষ্য করে [১] !"
        }
      ]
    },
    {
      "id": 5783,
      "verse_number": 25,
      "verse_key": "80:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا",
      "text_imlaei": "أَنَّا صَبَبْنَا الْمَاءَ صَبًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101820,
          "resource_id": 20,
          "text": "How We poured down water in torrents,"
        },
        {
          "id": 1172379,
          "resource_id": 213,
          "text": "নিশ্চয় আমরা প্রচুর বারি বর্ষণ করি,"
        }
      ]
    },
    {
      "id": 5784,
      "verse_number": 26,
      "verse_key": "80:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا",
      "text_imlaei": "ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101821,
          "resource_id": 20,
          "text": "Then We broke open the earth, splitting [it with sprouts],"
        },
        {
          "id": 1172380,
          "resource_id": 213,
          "text": "তারপর আমরা যমীনকে যথাযথভাবে বিদীর্ণ করি;"
        }
      ]
    },
    {
      "id": 5785,
      "verse_number": 27,
      "verse_key": "80:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنۢبَتْنَا فِيهَا حَبًّا",
      "text_imlaei": "فَأَنبَتْنَا فِيهَا حَبًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101822,
          "resource_id": 20,
          "text": "And caused to grow within it grain"
        },
        {
          "id": 1172381,
          "resource_id": 213,
          "text": "অতঃপর তাতে আমরা উৎপন্ন করি শস্য;"
        }
      ]
    },
    {
      "id": 5786,
      "verse_number": 28,
      "verse_key": "80:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَعِنَبًا وَقَضْبًا",
      "text_imlaei": "وَعِنَبًا وَقَضْبًا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101823,
          "resource_id": 20,
          "text": "And grapes and herbage"
        },
        {
          "id": 1172382,
          "resource_id": 213,
          "text": "আঙ্গুর, শাক-সব্জি,"
        }
      ]
    },
    {
      "id": 5787,
      "verse_number": 29,
      "verse_key": "80:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَزَيْتُونًا وَنَخْلًا",
      "text_imlaei": "وَزَيْتُونًا وَنَخْلًا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101824,
          "resource_id": 20,
          "text": "And olive and palm trees"
        },
        {
          "id": 1172383,
          "resource_id": 213,
          "text": "যায়তূন, খেজুরগাছ,"
        }
      ]
    },
    {
      "id": 5788,
      "verse_number": 30,
      "verse_key": "80:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَحَدَآئِقَ غُلْبًا",
      "text_imlaei": "وَحَدَائِقَ غُلْبًا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101826,
          "resource_id": 20,
          "text": "And gardens of dense shrubbery"
        },
        {
          "id": 1172384,
          "resource_id": 213,
          "text": "অনেক গাছবিশিষ্ট উদ্যান,"
        }
      ]
    },
    {
      "id": 5789,
      "verse_number": 31,
      "verse_key": "80:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفَـٰكِهَةً وَأَبًّا",
      "text_imlaei": "وَفَاكِهَةً وَأَبًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101827,
          "resource_id": 20,
          "text": "And fruit and grass -"
        },
        {
          "id": 1172385,
          "resource_id": 213,
          "text": "ফল এবং গবাদি খাদ্য [১],"
        }
      ]
    },
    {
      "id": 5790,
      "verse_number": 32,
      "verse_key": "80:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "text_imlaei": "مَّتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101828,
          "resource_id": 20,
          "text": "[As] enjoyment [i.e., provision] for you and your grazing livestock."
        },
        {
          "id": 1172386,
          "resource_id": 213,
          "text": "এগুলো তোমাদের ও তোমাদের চতুস্পদ জন্তুদের ভোগের জন্য [১]।"
        }
      ]
    },
    {
      "id": 5791,
      "verse_number": 33,
      "verse_key": "80:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "text_imlaei": "فَإِذَا جَاءَتِ الصَّاخَّةُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101829,
          "resource_id": 20,
          "text": "But when there comes the Deafening Blast<sup foot_note=197712>1</sup>"
        },
        {
          "id": 1172387,
          "resource_id": 213,
          "text": "অতঃপর যখন তীক্ষ্ন আওয়াজ আসবে [১],"
        }
      ]
    },
    {
      "id": 5792,
      "verse_number": 34,
      "verse_key": "80:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
      "text_imlaei": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101830,
          "resource_id": 20,
          "text": "On the Day a man will flee from his brother"
        },
        {
          "id": 1172388,
          "resource_id": 213,
          "text": "সেদিন মানুষ পালিয়ে যাবে তার ভাইয়ের কাছ থেকে"
        }
      ]
    },
    {
      "id": 5793,
      "verse_number": 35,
      "verse_key": "80:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأُمِّهِۦ وَأَبِيهِ",
      "text_imlaei": "وَأُمِّهِ وَأَبِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101831,
          "resource_id": 20,
          "text": "And his mother and his father"
        },
        {
          "id": 1172389,
          "resource_id": 213,
          "text": "এবং তার মাতা, তার পিতা,"
        }
      ]
    },
    {
      "id": 5794,
      "verse_number": 36,
      "verse_key": "80:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَصَـٰحِبَتِهِۦ وَبَنِيهِ",
      "text_imlaei": "وَصَاحِبَتِهِ وَبَنِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101832,
          "resource_id": 20,
          "text": "And his wife and his children,"
        },
        {
          "id": 1172390,
          "resource_id": 213,
          "text": "তার পত্নী ও তার সন্তান থেকে [১],"
        }
      ]
    },
    {
      "id": 5795,
      "verse_number": 37,
      "verse_key": "80:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "text_imlaei": "لِكُلِّ امْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101833,
          "resource_id": 20,
          "text": "For every man, that Day, will be a matter adequate for him.<sup foot_note=197713>1</sup>"
        },
        {
          "id": 1172391,
          "resource_id": 213,
          "text": "সেদিন তাদের প্রত্যেকের হবে এমন গুরুতর অবস্থা যা তাকে সম্পূর্ণরূপে ব্যস্ত রাখবে [১]।"
        }
      ]
    },
    {
      "id": 5796,
      "verse_number": 38,
      "verse_key": "80:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
      "text_imlaei": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101834,
          "resource_id": 20,
          "text": "[Some] faces, that Day, will be bright -"
        },
        {
          "id": 1172392,
          "resource_id": 213,
          "text": "অনেক চেহারা সেদিন হবে উজ্জ্বল,"
        }
      ]
    },
    {
      "id": 5797,
      "verse_number": 39,
      "verse_key": "80:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
      "text_imlaei": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101835,
          "resource_id": 20,
          "text": "Laughing, rejoicing at good news."
        },
        {
          "id": 1172393,
          "resource_id": 213,
          "text": "সহাস্য ও প্রফুল্ল,"
        }
      ]
    },
    {
      "id": 5798,
      "verse_number": 40,
      "verse_key": "80:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "text_imlaei": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101837,
          "resource_id": 20,
          "text": "And [other] faces, that Day, will have upon them dust."
        },
        {
          "id": 1172394,
          "resource_id": 213,
          "text": "আর অনেক চেহারা সেদিন হবে ধূলিধূসর"
        }
      ]
    },
    {
      "id": 5799,
      "verse_number": 41,
      "verse_key": "80:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَرْهَقُهَا قَتَرَةٌ",
      "text_imlaei": "تَرْهَقُهَا قَتَرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101838,
          "resource_id": 20,
          "text": "Blackness will cover them."
        },
        {
          "id": 1172395,
          "resource_id": 213,
          "text": "সেগুলোকে আচ্ছন্ন করবে কালিমা।"
        }
      ]
    },
    {
      "id": 5800,
      "verse_number": 42,
      "verse_key": "80:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أُو۟لَـٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
      "text_imlaei": "أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 101839,
          "resource_id": 20,
          "text": "Those are the disbelievers, the wicked ones."
        },
        {
          "id": 1172396,
          "resource_id": 213,
          "text": "এরাই কাফির ও পাপাচারী।"
        }
      ]
    }
  ],
