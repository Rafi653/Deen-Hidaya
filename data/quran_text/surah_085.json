    ],
    "translated_name": {
      "language_name": "english",
      "name": "The Mansions of the Stars"
    }
  },
  "verses": [
    {
      "id": 5910,
      "verse_number": 1,
      "verse_key": "85:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
      "text_imlaei": "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102003,
          "resource_id": 20,
          "text": "By the sky containing great stars"
        },
        {
          "id": 1172506,
          "resource_id": 213,
          "text": "শপথ বুরূজবিশিষ্ট [১] আসমানের,"
        }
      ]
    },
    {
      "id": 5911,
      "verse_number": 2,
      "verse_key": "85:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
      "text_imlaei": "وَالْيَوْمِ الْمَوْعُودِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102015,
          "resource_id": 20,
          "text": "And [by] the promised Day"
        },
        {
          "id": 1172507,
          "resource_id": 213,
          "text": "আর প্রতিশ্রুত দিনের"
        }
      ]
    },
    {
      "id": 5912,
      "verse_number": 3,
      "verse_key": "85:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَشَاهِدٍ وَمَشْهُودٍ",
      "text_imlaei": "وَشَاهِدٍ وَمَشْهُودٍ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102020,
          "resource_id": 20,
          "text": "And [by] the witness and what is witnessed,"
        },
        {
          "id": 1172508,
          "resource_id": 213,
          "text": "এবং দ্রষ্টা ও দৃষ্টের [১]---"
        }
      ]
    },
    {
      "id": 5913,
      "verse_number": 4,
      "verse_key": "85:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُتِلَ أَصْحَـٰبُ ٱلْأُخْدُودِ",
      "text_imlaei": "قُتِلَ أَصْحَابُ الْأُخْدُودِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102022,
          "resource_id": 20,
          "text": "Destroyed [i.e., cursed] were the companions of the trench<sup foot_note=197740>1</sup>"
        },
        {
          "id": 1172509,
          "resource_id": 213,
          "text": "অভিশপ্ত হয়েছিল [১] কুণ্ডের অধিপতিরা--- [২]"
        }
      ]
    },
    {
      "id": 5914,
      "verse_number": 5,
      "verse_key": "85:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
      "text_imlaei": "النَّارِ ذَاتِ الْوَقُودِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102024,
          "resource_id": 20,
          "text": "[Containing] the fire full of fuel,"
        },
        {
          "id": 1172510,
          "resource_id": 213,
          "text": "যে কুণ্ডে ছিল ইন্ধনপূর্ণ আগুন,"
        }
      ]
    },
    {
      "id": 5915,
      "verse_number": 6,
      "verse_key": "85:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
      "text_imlaei": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102026,
          "resource_id": 20,
          "text": "When they were sitting near it."
        },
        {
          "id": 1172511,
          "resource_id": 213,
          "text": "যখন তারা এর পাশে উপবিষ্ট ছিল"
        }
      ]
    },
    {
      "id": 5916,
      "verse_number": 7,
      "verse_key": "85:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ",
      "text_imlaei": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102028,
          "resource_id": 20,
          "text": "And they, to what they were doing against the believers, were witnesses. <sup foot_note=197741>1</sup>"
        },
        {
          "id": 1172512,
          "resource_id": 213,
          "text": "এবং তারা মুমিনদের সাথে যা করছিল তা প্রত্যক্ষ করছিল।"
        }
      ]
    },
    {
      "id": 5917,
      "verse_number": 8,
      "verse_key": "85:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
      "text_imlaei": "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَن يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102030,
          "resource_id": 20,
          "text": "And they resented them not except because they believed in Allāh, the Exalted in Might, the Praiseworthy,"
        },
        {
          "id": 1172513,
          "resource_id": 213,
          "text": "আর তারা তাদেরকে নির্যাতন করেছিল শুধু এ কারণে যে, তারা ঈমান এনেছিল পরাক্রমশালী ও প্রশংসার যোগ্য আল্লাহর উপর---"
        }
      ]
    },
    {
      "id": 5918,
      "verse_number": 9,
      "verse_key": "85:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ",
      "text_imlaei": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102032,
          "resource_id": 20,
          "text": "To whom belongs the dominion of the heavens and the earth. And Allāh, over all things, is Witness.<sup foot_note=197742>1</sup>"
        },
        {
          "id": 1172514,
          "resource_id": 213,
          "text": "আসমানমূহ ও যমীনের সর্বময় কর্তৃত্ব যাঁর; আর আল্লাহ্ সবকিছুর প্রত্যক্ষদর্শী।"
        }
      ]
    },
    {
      "id": 5919,
      "verse_number": 10,
      "verse_key": "85:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَـٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
      "text_imlaei": "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102004,
          "resource_id": 20,
          "text": "Indeed, those who have tortured<sup foot_note=197743>1</sup> the believing men and believing women and then have not repented will have the punishment of Hell, and they will have the punishment of the Burning Fire."
        },
        {
          "id": 1172515,
          "resource_id": 213,
          "text": "নিশ্চয় যারা মুমিন নর-নারীকে বিপদাপন্ন করেছে [১] তারপর তাওবা করেনি [২]  তাদের জন্য আছে জাহান্নামের শাস্তি, আর তাদের জন্য আছে দহন যন্ত্রণা [৩]।"
        }
      ]
    },
    {
      "id": 5920,
      "verse_number": 11,
      "verse_key": "85:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ جَنَّـٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
      "text_imlaei": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِن تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102005,
          "resource_id": 20,
          "text": "Indeed, those who have believed and done righteous deeds will have gardens beneath which rivers flow. That is the great attainment."
        },
        {
          "id": 1172516,
          "resource_id": 213,
          "text": "নিশ্চয় যারা ঈমান এনেছে এবং সৎকাজ করেছে তাদের জন্য রয়েছে জান্নাত, যার পাদদেশে নদী প্রবাহিত; এটাই মহাসাফল্য।"
        }
      ]
    },
    {
      "id": 5921,
      "verse_number": 12,
      "verse_key": "85:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
      "text_imlaei": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102006,
          "resource_id": 20,
          "text": "Indeed, the assault [i.e., vengeance] of your Lord is severe."
        },
        {
          "id": 1172517,
          "resource_id": 213,
          "text": "নিশ্চয় আপনার রবের পাকড়াও বড়ই কঠিন।"
        }
      ]
    },
    {
      "id": 5922,
      "verse_number": 13,
      "verse_key": "85:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
      "text_imlaei": "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102007,
          "resource_id": 20,
          "text": "Indeed, it is He who originates [creation] and repeats."
        },
        {
          "id": 1172518,
          "resource_id": 213,
          "text": "তিনিই অস্তিত্ব দান করেন ও পুনরাবর্তন ঘটান"
        }
      ]
    },
    {
      "id": 5923,
      "verse_number": 14,
      "verse_key": "85:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
      "text_imlaei": "وَهُوَ الْغَفُورُ الْوَدُودُ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102008,
          "resource_id": 20,
          "text": "And He is the Forgiving, the Affectionate,"
        },
        {
          "id": 1172519,
          "resource_id": 213,
          "text": "এবং তিনি ক্ষমাশীল, অতিস্নেহময় [১],"
        }
      ]
    },
    {
      "id": 5924,
      "verse_number": 15,
      "verse_key": "85:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
      "text_imlaei": "ذُو الْعَرْشِ الْمَجِيدُ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102009,
          "resource_id": 20,
          "text": "Honorable Owner of the Throne,"
        },
        {
          "id": 1172520,
          "resource_id": 213,
          "text": "আরশের অধিকারী ও সম্মানিত।"
        }
      ]
    },
    {
      "id": 5925,
      "verse_number": 16,
      "verse_key": "85:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَعَّالٌ لِّمَا يُرِيدُ",
      "text_imlaei": "فَعَّالٌ لِّمَا يُرِيدُ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102010,
          "resource_id": 20,
          "text": "Effecter of what He intends."
        },
        {
          "id": 1172521,
          "resource_id": 213,
          "text": "তিনি যা ইচ্ছে তা-ই করেন [১]।"
        }
      ]
    },
    {
      "id": 5926,
      "verse_number": 17,
      "verse_key": "85:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
      "text_imlaei": "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ",
      "page_number": 590,
      "juz_number": 30,
      "translations": [
        {
          "id": 102011,
          "resource_id": 20,
          "text": "Has there reached you the story of the soldiers -"
        },
        {
          "id": 1172522,
          "resource_id": 213,
          "text": "আপনার কাছে কি পৌঁছেছে সৈন্যবাহিনীর বৃত্তান্ত---"
        }
      ]
    },
    {
      "id": 5927,
      "verse_number": 18,
      "verse_key": "85:18",
