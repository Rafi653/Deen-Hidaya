    "translated_name": {
      "language_name": "english",
      "name": "The Rivalry in world increase"
    }
  },
  "verses": [
    {
      "id": 6169,
      "verse_number": 1,
      "verse_key": "102:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " أَلْهَىٰكُمُ ٱلتَّكَاثُرُ",
      "text_imlaei": "أَلْهَاكُمُ التَّكَاثُرُ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96211,
          "resource_id": 20,
          "text": "Competition in [worldly] increase diverts you"
        },
        {
          "id": 1172765,
          "resource_id": 213,
          "text": "তোমাদেরকে মোহাচ্ছন্ন রাখে [১] প্রাচুর্যের প্রতিযোগিতা [২]"
        }
      ]
    },
    {
      "id": 6170,
      "verse_number": 2,
      "verse_key": "102:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "حَتَّىٰ زُرْتُمُ ٱلْمَقَابِرَ",
      "text_imlaei": "حَتَّىٰ زُرْتُمُ الْمَقَابِرَ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96213,
          "resource_id": 20,
          "text": "Until you visit the graveyards.<sup foot_note=197808>1</sup>"
        },
        {
          "id": 1172766,
          "resource_id": 213,
          "text": "যতক্ষণ না তোমরা কবরে উপনীত হও [১]।"
        }
      ]
    },
    {
      "id": 6171,
      "verse_number": 3,
      "verse_key": "102:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا سَوْفَ تَعْلَمُونَ",
      "text_imlaei": "كَلَّا سَوْفَ تَعْلَمُونَ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96215,
          "resource_id": 20,
          "text": "No! You are going to know."
        },
        {
          "id": 1172767,
          "resource_id": 213,
          "text": "কখনো নয়, তোমরা শীঘ্রই জানতে পারবে [১];"
        }
      ]
    },
    {
      "id": 6172,
      "verse_number": 4,
      "verse_key": "102:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ",
      "text_imlaei": "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96217,
          "resource_id": 20,
          "text": "Then, no! You are going to know."
        },
        {
          "id": 1172768,
          "resource_id": 213,
          "text": "তারপর, কখনো নয়, তোমরা শীঘ্রই জানতে পারবে:"
        }
      ]
    },
    {
      "id": 6173,
      "verse_number": 5,
      "verse_key": "102:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ ٱلْيَقِينِ",
      "text_imlaei": "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ الْيَقِينِ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96219,
          "resource_id": 20,
          "text": "No! If you only knew with knowledge of certainty...<sup foot_note=197809>1</sup>"
        },
        {
          "id": 1172769,
          "resource_id": 213,
          "text": "কখনো নয়! যদি তোমরা নিশ্চিত জ্ঞানে জ্ঞানী হতে--- [১]"
        }
      ]
    },
    {
      "id": 6174,
      "verse_number": 6,
      "verse_key": "102:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَتَرَوُنَّ ٱلْجَحِيمَ",
      "text_imlaei": "لَتَرَوُنَّ الْجَحِيمَ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96221,
          "resource_id": 20,
          "text": "You will surely see the Hellfire."
        },
        {
          "id": 1172770,
          "resource_id": 213,
          "text": "অবশ্যই তোমরা জাহান্নাম দেখবে;"
        }
      ]
    },
    {
      "id": 6175,
      "verse_number": 7,
      "verse_key": "102:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ لَتَرَوُنَّهَا عَيْنَ ٱلْيَقِينِ",
      "text_imlaei": "ثُمَّ لَتَرَوُنَّهَا عَيْنَ الْيَقِينِ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96223,
          "resource_id": 20,
          "text": "Then you will surely see it with the eye of certainty.<sup foot_note=197810>1</sup>"
        },
        {
          "id": 1172771,
          "resource_id": 213,
          "text": "তারপর অবশ্যই তোমরা তা দেখবে চাক্ষুষ প্রত্যয়ে [১],"
        }
      ]
    },
    {
      "id": 6176,
      "verse_number": 8,
      "verse_key": "102:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 546,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ لَتُسْـَٔلُنَّ يَوْمَئِذٍ عَنِ ٱلنَّعِيمِ",
      "text_imlaei": "ثُمَّ لَتُسْأَلُنَّ يَوْمَئِذٍ عَنِ النَّعِيمِ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 96225,
          "resource_id": 20,
          "text": "Then you will surely be asked that Day about pleasure.<sup foot_note=197811>1</sup>"
        },
        {
          "id": 1172772,
          "resource_id": 213,
          "text": "তারপর অবশ্যই সেদিন তোমাদেরকে নেয়ামত সম্পর্কে প্রশ্ন করা হবে [১]।"
        }
      ]
    }
  ],
