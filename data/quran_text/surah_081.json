  "surah_info": {
    "id": 81,
    "revelation_place": "makkah",
    "revelation_order": 7,
    "bismillah_pre": true,
    "name_simple": "At-Takwir",
    "name_complex": "At-Takwīr",
    "name_arabic": "التكوير",
    "verses_count": 29,
    "pages": [
      586,
      586
    ],
    "translated_name": {
      "language_name": "english",
      "name": "The Overthrowing"
    }
  },
  "verses": [
    {
      "id": 5801,
      "verse_number": 1,
      "verse_key": "81:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " إِذَا ٱلشَّمْسُ كُوِّرَتْ",
      "text_imlaei": "إِذَا الشَّمْسُ كُوِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101848,
          "resource_id": 20,
          "text": "When the sun is wrapped up [in darkness]"
        },
        {
          "id": 1172397,
          "resource_id": 213,
          "text": "সূর্যকে যখন নিষ্প্রভ করা হবে [১],"
        }
      ]
    },
    {
      "id": 5802,
      "verse_number": 2,
      "verse_key": "81:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
      "text_imlaei": "وَإِذَا النُّجُومُ انكَدَرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101860,
          "resource_id": 20,
          "text": "And when the stars fall, dispersing,"
        },
        {
          "id": 1172398,
          "resource_id": 213,
          "text": "আর যখন নক্ষত্ররাজি খসে পড়বে [১],"
        }
      ]
    },
    {
      "id": 5803,
      "verse_number": 3,
      "verse_key": "81:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
      "text_imlaei": "وَإِذَا الْجِبَالُ سُيِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101872,
          "resource_id": 20,
          "text": "And when the mountains are removed"
        },
        {
          "id": 1172399,
          "resource_id": 213,
          "text": "আর পর্বতসমূহকে যখন চলমান করা হবে [১],"
        }
      ]
    },
    {
      "id": 5804,
      "verse_number": 4,
      "verse_key": "81:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
      "text_imlaei": "وَإِذَا الْعِشَارُ عُطِّلَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101873,
          "resource_id": 20,
          "text": "And when full-term she-camels<sup foot_note=197714>1</sup> are neglected"
        },
        {
          "id": 1172400,
          "resource_id": 213,
          "text": "আর যখন পূর্ণগর্ভা মাদী উট উপেক্ষিত হবে [১] ,"
        }
      ]
    },
    {
      "id": 5805,
      "verse_number": 5,
      "verse_key": "81:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
      "text_imlaei": "وَإِذَا الْوُحُوشُ حُشِرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101875,
          "resource_id": 20,
          "text": "And when the wild beasts are gathered"
        },
        {
          "id": 1172401,
          "resource_id": 213,
          "text": "আর যখন বন্য পশুগুলো একত্র করা হবে,"
        }
      ]
    },
    {
      "id": 5806,
      "verse_number": 6,
      "verse_key": "81:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
      "text_imlaei": "وَإِذَا الْبِحَارُ سُجِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101877,
          "resource_id": 20,
          "text": "And when the seas are filled with flame<sup foot_note=197715>1</sup>"
        },
        {
          "id": 1172402,
          "resource_id": 213,
          "text": "আর যখন সাগরকে অগ্নি-উত্তাল করা হবে [১],"
        }
      ]
    },
    {
      "id": 5807,
      "verse_number": 7,
      "verse_key": "81:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
      "text_imlaei": "وَإِذَا النُّفُوسُ زُوِّجَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101879,
          "resource_id": 20,
          "text": "And when the souls are paired<sup foot_note=197716>1</sup>"
        },
        {
          "id": 1172403,
          "resource_id": 213,
          "text": "আর যখন আত্মাগুলোকে সমগোত্রীয়দের সাথে মিলিয়ে দেয়া হবে [১] ,"
        }
      ]
    },
    {
      "id": 5808,
      "verse_number": 8,
      "verse_key": "81:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
      "text_imlaei": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101881,
          "resource_id": 20,
          "text": "And when the girl [who was] buried alive is asked"
        },
        {
          "id": 1172404,
          "resource_id": 213,
          "text": "আর যখন জীবন্ত সমাধিস্থ কন্যাকে [১] জিজ্ঞেস করা হবে,"
        }
      ]
    },
    {
      "id": 5809,
      "verse_number": 9,
      "verse_key": "81:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بِأَىِّ ذَنۢبٍ قُتِلَتْ",
      "text_imlaei": "بِأَيِّ ذَنبٍ قُتِلَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101883,
          "resource_id": 20,
          "text": "For what sin she was killed"
        },
        {
          "id": 1172405,
          "resource_id": 213,
          "text": "কী অপরাধে তাকে হত্যা করা হয়েছিল [১]?"
        }
      ]
    },
    {
      "id": 5810,
      "verse_number": 10,
      "verse_key": "81:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
      "text_imlaei": "وَإِذَا الصُّحُفُ نُشِرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101849,
          "resource_id": 20,
          "text": "And when the pages<sup foot_note=197717>1</sup> are spread [i.e., made public]"
        },
        {
          "id": 1172406,
          "resource_id": 213,
          "text": "আর যখন আমলনামাগুলো উন্মোচিত করা হবে,"
        }
      ]
    },
    {
      "id": 5811,
      "verse_number": 11,
      "verse_key": "81:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
      "text_imlaei": "وَإِذَا السَّمَاءُ كُشِطَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101850,
          "resource_id": 20,
          "text": "And when the sky is stripped away"
        },
        {
          "id": 1172407,
          "resource_id": 213,
          "text": "আর যখন আসমানের আবরণ অপসারিত করা হবে [১],"
        }
      ]
    },
    {
      "id": 5812,
      "verse_number": 12,
      "verse_key": "81:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
      "text_imlaei": "وَإِذَا الْجَحِيمُ سُعِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101851,
          "resource_id": 20,
          "text": "And when Hellfire is set ablaze"
        },
        {
          "id": 1172408,
          "resource_id": 213,
          "text": "আর যখন জাহান্নামকে ভীষণভাবে প্ৰজ্জ্বলিত করা হবে,"
        }
      ]
    },
    {
      "id": 5813,
      "verse_number": 13,
      "verse_key": "81:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
      "text_imlaei": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101852,
          "resource_id": 20,
          "text": "And when Paradise is brought near,"
        },
        {
          "id": 1172409,
          "resource_id": 213,
          "text": "আর যখন জান্নাত নিকটবর্তী করা হবে,"
        }
      ]
    },
    {
      "id": 5814,
      "verse_number": 14,
      "verse_key": "81:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ",
      "text_imlaei": "عَلِمَتْ نَفْسٌ مَّا أَحْضَرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101853,
          "resource_id": 20,
          "text": "A soul will [then] know what it has brought [with it].<sup foot_note=197718>1</sup>"
        },
        {
          "id": 1172410,
          "resource_id": 213,
          "text": "তখন প্ৰত্যেক ব্যক্তিই জানবে সে কি উপস্থিত করেছে [১]।"
        }
      ]
    },
    {
      "id": 5815,
      "verse_number": 15,
      "verse_key": "81:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
      "text_imlaei": "فَلَا أُقْسِمُ بِالْخُنَّسِ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101854,
          "resource_id": 20,
          "text": "So I swear by the retreating stars -"
        },
        {
          "id": 1172411,
          "resource_id": 213,
          "text": "সুতরাং আমি শপথ করছি পশ্চাদপসরণকারী নক্ষত্রের,"
        }
      ]
    },
    {
      "id": 5816,
      "verse_number": 16,
      "verse_key": "81:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلْجَوَارِ ٱلْكُنَّسِ",
      "text_imlaei": "الْجَوَارِ الْكُنَّسِ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101855,
          "resource_id": 20,
          "text": "Those that run [their courses] and disappear [i.e., set]"
        },
        {
          "id": 1172412,
          "resource_id": 213,
          "text": "যা চলমান, অপসৃয়মাণ,"
        }
      ]
    },
    {
      "id": 5817,
      "verse_number": 17,
      "verse_key": "81:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
      "text_imlaei": "وَاللَّيْلِ إِذَا عَسْعَسَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101856,
          "resource_id": 20,
          "text": "And by the night as it closes in<sup foot_note=197719>1</sup>"
        },
        {
          "id": 1172413,
          "resource_id": 213,
          "text": "শপথ রাতের যখন তা শেষ হয় [১]"
        }
      ]
    },
    {
      "id": 5818,
      "verse_number": 18,
      "verse_key": "81:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
      "text_imlaei": "وَالصُّبْحِ إِذَا تَنَفَّسَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101857,
          "resource_id": 20,
          "text": "And by the dawn when it breathes [i.e., stirs]"
        },
        {
          "id": 1172414,
          "resource_id": 213,
          "text": "শপথ প্ৰভাতের যখন তার আবির্ভাব হয়,"
        }
      ]
    },
    {
      "id": 5819,
      "verse_number": 19,
      "verse_key": "81:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "text_imlaei": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101858,
          "resource_id": 20,
          "text": "[That] indeed, it [i.e., the Qur’ān] is a word [conveyed by] a noble messenger [i.e., Gabriel]"
        },
        {
          "id": 1172415,
          "resource_id": 213,
          "text": "নিশ্চয়ই এ কুরআন সম্মানিত রাসূলের আনীত বাণী [১]"
        }
      ]
    },
    {
      "id": 5820,
      "verse_number": 20,
      "verse_key": "81:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ",
      "text_imlaei": "ذِي قُوَّةٍ عِندَ ذِي الْعَرْشِ مَكِينٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101861,
          "resource_id": 20,
          "text": "[Who is] possessed of power and with the Owner of the Throne, secure [in position],"
        },
        {
          "id": 1172416,
          "resource_id": 213,
          "text": "যে সামর্থ্যশালী, আরশের মালিকের কাছে মর্যাদা সম্পন্ন,"
        }
      ]
    },
    {
      "id": 5821,
      "verse_number": 21,
      "verse_key": "81:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مُّطَاعٍ ثَمَّ أَمِينٍ",
      "text_imlaei": "مُّطَاعٍ ثَمَّ أَمِينٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101862,
          "resource_id": 20,
          "text": "Obeyed there [in the heavens] and trustworthy."
        },
        {
          "id": 1172417,
          "resource_id": 213,
          "text": "সে মান্য সেখানে, বিশ্বাসভাজন [১]।"
        }
      ]
    },
    {
      "id": 5822,
      "verse_number": 22,
      "verse_key": "81:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا صَاحِبُكُم بِمَجْنُونٍ",
      "text_imlaei": "وَمَا صَاحِبُكُم بِمَجْنُونٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101863,
          "resource_id": 20,
          "text": "And your companion [i.e., Prophet Muḥammad (ﷺ) ] is not [at all] mad.<sup foot_note=197720>1</sup>"
        },
        {
          "id": 1172418,
          "resource_id": 213,
          "text": "আর তোমাদের সাথী উন্মাদ নন [১],"
        }
      ]
    },
    {
      "id": 5823,
      "verse_number": 23,
      "verse_key": "81:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
      "text_imlaei": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101864,
          "resource_id": 20,
          "text": "And he has already seen him [i.e., Gabriel] in the clear horizon.<sup foot_note=197721>1</sup>"
        },
        {
          "id": 1172419,
          "resource_id": 213,
          "text": "তিনি তো তাকে স্পষ্ট দিগন্তে দেখেছেন [১],"
        }
      ]
    },
    {
      "id": 5824,
      "verse_number": 24,
      "verse_key": "81:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ",
      "text_imlaei": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101865,
          "resource_id": 20,
          "text": "And he [i.e., Muḥammad (ﷺ)] is not a withholder of [knowledge of] the unseen.<sup foot_note=197722>1</sup>"
        },
        {
          "id": 1172420,
          "resource_id": 213,
          "text": "তিনি গায়েবী বিষয় সম্পর্কে কৃপণ নয় [১]।"
        }
      ]
    },
    {
      "id": 5825,
      "verse_number": 25,
      "verse_key": "81:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا هُوَ بِقَوْلِ شَيْطَـٰنٍ رَّجِيمٍ",
      "text_imlaei": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَّجِيمٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101866,
          "resource_id": 20,
          "text": "And it [i.e., the Qur’ān] is not the word of a devil, expelled [from the heavens]."
        },
        {
          "id": 1172421,
          "resource_id": 213,
          "text": "আর এটা কোনো অভিশপ্ত শয়তানের বাক্য নয়।"
        }
      ]
    },
    {
      "id": 5826,
      "verse_number": 26,
      "verse_key": "81:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَيْنَ تَذْهَبُونَ",
      "text_imlaei": "فَأَيْنَ تَذْهَبُونَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101867,
          "resource_id": 20,
          "text": "So where are you going?<sup foot_note=197723>1</sup>"
        },
        {
          "id": 1172422,
          "resource_id": 213,
          "text": "কাজেই তোমরা কোথায় যাচ্ছ?!"
        }
      ]
    },
    {
      "id": 5827,
      "verse_number": 27,
      "verse_key": "81:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ",
      "text_imlaei": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَالَمِينَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101868,
          "resource_id": 20,
          "text": "It is not except a reminder to the worlds"
        },
        {
          "id": 1172423,
          "resource_id": 213,
          "text": "এ তো শুধু সৃষ্টিকুলের জন্য উপদেশ,"
        }
      ]
    },
    {
      "id": 5828,
      "verse_number": 28,
      "verse_key": "81:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
      "text_imlaei": "لِمَن شَاءَ مِنكُمْ أَن يَسْتَقِيمَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101869,
          "resource_id": 20,
          "text": "For whoever wills among you to take a right course."
        },
        {
          "id": 1172424,
          "resource_id": 213,
          "text": "তোমাদের মধ্যে যে সরল পথে চলতে চায়, তার জন্য [১]।"
        }
      ]
    },
    {
      "id": 5829,
      "verse_number": 29,
      "verse_key": "81:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَـٰلَمِينَ",
      "text_imlaei": "وَمَا تَشَاءُونَ إِلَّا أَن يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 101870,
          "resource_id": 20,
          "text": "And you do not will except that Allāh wills - Lord of the worlds."
        },
        {
          "id": 1172425,
          "resource_id": 213,
          "text": "আর তোমরা ইচ্ছে করতে পার না, যদি না সৃষ্টিকুলের রব আল্লাহ্ ইচ্ছে করেন [১]।"
        }
      ]
    }
  ],
