  "surah_info": {
    "id": 75,
    "revelation_place": "makkah",
    "revelation_order": 31,
    "bismillah_pre": true,
    "name_simple": "Al-Qiyamah",
    "name_complex": "Al-Qiyāmah",
    "name_arabic": "القيامة",
    "verses_count": 40,
    "pages": [
      577,
      578
    ],
    "translated_name": {
      "language_name": "english",
      "name": "The Resurrection"
    }
  },
  "verses": [
    {
      "id": 5552,
      "verse_number": 1,
      "verse_key": "75:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَـٰمَةِ",
      "text_imlaei": "لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101547,
          "resource_id": 20,
          "text": "I swear by the Day of Resurrection"
        },
        {
          "id": 1172148,
          "resource_id": 213,
          "text": "আমি শপথ করছি কিয়ামতের দিনের [১],"
        }
      ]
    },
    {
      "id": 5553,
      "verse_number": 2,
      "verse_key": "75:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ",
      "text_imlaei": "وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101559,
          "resource_id": 20,
          "text": "And I swear by the reproaching soul<sup foot_note=197655>1</sup> [to the certainty of resurrection]."
        },
        {
          "id": 1172149,
          "resource_id": 213,
          "text": "আমি আরও শপথ করছি ভর্ৎসনাকারী আত্মার [১]।"
        }
      ]
    },
    {
      "id": 5554,
      "verse_number": 3,
      "verse_key": "75:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَيَحْسَبُ ٱلْإِنسَـٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ",
      "text_imlaei": "أَيَحْسَبُ الْإِنسَانُ أَلَّن نَّجْمَعَ عِظَامَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101571,
          "resource_id": 20,
          "text": "Does man think that We will not assemble his bones?"
        },
        {
          "id": 1172150,
          "resource_id": 213,
          "text": "মানুষ কি মনে করে যে, আমরা কখনোই তার অস্থিসমূহ একত্র করতে পারব না?"
        }
      ]
    },
    {
      "id": 5555,
      "verse_number": 4,
      "verse_key": "75:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلَىٰ قَـٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ",
      "text_imlaei": "بَلَىٰ قَادِرِينَ عَلَىٰ أَن نُّسَوِّيَ بَنَانَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101583,
          "resource_id": 20,
          "text": "Yes. [We are] Able [even] to proportion his fingertips."
        },
        {
          "id": 1172151,
          "resource_id": 213,
          "text": "অবশ্যই হ্যাঁ, আমরা তার আঙ্গুলের আগা পর্যন্ত পুনর্বিন্যস্ত করতে সক্ষম [১]।"
        }
      ]
    },
    {
      "id": 5556,
      "verse_number": 5,
      "verse_key": "75:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلْ يُرِيدُ ٱلْإِنسَـٰنُ لِيَفْجُرَ أَمَامَهُۥ",
      "text_imlaei": "بَلْ يُرِيدُ الْإِنسَانُ لِيَفْجُرَ أَمَامَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101586,
          "resource_id": 20,
          "text": "But man desires to continue in sin.<sup foot_note=197656>1</sup>"
        },
        {
          "id": 1172152,
          "resource_id": 213,
          "text": "বরং মানুষ তার ভবিষ্যতেও পাপাচার করতে চায় [১]।"
        }
      ]
    },
    {
      "id": 5557,
      "verse_number": 6,
      "verse_key": "75:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَـٰمَةِ",
      "text_imlaei": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101588,
          "resource_id": 20,
          "text": "He asks, \"When is the Day of Resurrection?\""
        },
        {
          "id": 1172153,
          "resource_id": 213,
          "text": "সে প্রশ্ন করে, ‘কখন কিয়ামতের দিন আসবে?’"
        }
      ]
    },
    {
      "id": 5558,
      "verse_number": 7,
      "verse_key": "75:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا بَرِقَ ٱلْبَصَرُ",
      "text_imlaei": "فَإِذَا بَرِقَ الْبَصَرُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101591,
          "resource_id": 20,
          "text": "So when vision is dazzled."
        },
        {
          "id": 1172154,
          "resource_id": 213,
          "text": "যখন চোখ স্থির হয়ে যাবে [১],"
        }
      ]
    },
    {
      "id": 5559,
      "verse_number": 8,
      "verse_key": "75:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَخَسَفَ ٱلْقَمَرُ",
      "text_imlaei": "وَخَسَفَ الْقَمَرُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101595,
          "resource_id": 20,
          "text": "And the moon darkens."
        },
        {
          "id": 1172155,
          "resource_id": 213,
          "text": "এবং চাঁদ হয়ে পড়বে কিরণহীন [১],"
        }
      ]
    },
    {
      "id": 5560,
      "verse_number": 9,
      "verse_key": "75:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ",
      "text_imlaei": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101597,
          "resource_id": 20,
          "text": "And the sun and the moon are joined,"
        },
        {
          "id": 1172156,
          "resource_id": 213,
          "text": "আর যখন সূর্য ও চাঁদকে একত্র করা হবে [১] –"
        }
      ]
    },
    {
      "id": 5561,
      "verse_number": 10,
      "verse_key": "75:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَقُولُ ٱلْإِنسَـٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ",
      "text_imlaei": "يَقُولُ الْإِنسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101548,
          "resource_id": 20,
          "text": "Man will say on that Day, \"Where is the [place of] escape?\""
        },
        {
          "id": 1172157,
          "resource_id": 213,
          "text": "সেদিন মানুষ বলবে, ‘আজ পালানোর স্থান কোথায়?’"
        }
      ]
    },
    {
      "id": 5562,
      "verse_number": 11,
      "verse_key": "75:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا لَا وَزَرَ",
      "text_imlaei": "كَلَّا لَا وَزَرَ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101549,
          "resource_id": 20,
          "text": "No! There is no refuge."
        },
        {
          "id": 1172158,
          "resource_id": 213,
          "text": "কখনোই নয়, কোনো আশ্রয়স্থল নেই।"
        }
      ]
    },
    {
      "id": 5563,
      "verse_number": 12,
      "verse_key": "75:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ",
      "text_imlaei": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101550,
          "resource_id": 20,
          "text": "To your Lord, that Day, is the [place of] permanence."
        },
        {
          "id": 1172159,
          "resource_id": 213,
          "text": "সেদিন ঠাঁই হবে আপনার রবেরই কাছে।"
        }
      ]
    },
    {
      "id": 5564,
      "verse_number": 13,
      "verse_key": "75:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يُنَبَّؤُا۟ ٱلْإِنسَـٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "text_imlaei": "يُنَبَّأُ الْإِنسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101551,
          "resource_id": 20,
          "text": "Man will be informed that Day of what he sent ahead<sup foot_note=197658>1</sup> and kept back.<sup foot_note=197657>2</sup>"
        },
        {
          "id": 1172160,
          "resource_id": 213,
          "text": "সেদিন মানুষকে অবহিত করা হবে সে কী আগে পাঠিয়েছে ও কী পিছনে রেখে গেছে [১]।"
        }
      ]
    },
    {
      "id": 5565,
      "verse_number": 14,
      "verse_key": "75:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلِ ٱلْإِنسَـٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌ",
      "text_imlaei": "بَلِ الْإِنسَانُ عَلَىٰ نَفْسِهِ بَصِيرَةٌ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101552,
          "resource_id": 20,
          "text": "Rather, man, against himself, will be a witness,<sup foot_note=197659>1</sup>"
        },
        {
          "id": 1172161,
          "resource_id": 213,
          "text": "বরং মানুষ নিজের সম্পর্কে সম্যক অবগত [১] ,"
        }
      ]
    },
    {
      "id": 5566,
      "verse_number": 15,
      "verse_key": "75:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ",
      "text_imlaei": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101553,
          "resource_id": 20,
          "text": "Even if he presents his excuses."
        },
        {
          "id": 1172162,
          "resource_id": 213,
          "text": "যদিও সে নানা অজুহাতের অবতারণা করে।"
        }
      ]
    },
    {
      "id": 5567,
      "verse_number": 16,
      "verse_key": "75:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ",
      "text_imlaei": "لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101554,
          "resource_id": 20,
          "text": "Move not your tongue with it, [O Muḥammad], to hasten with it [i.e., recitation of the Qur’ān]."
        },
        {
          "id": 1172163,
          "resource_id": 213,
          "text": "তাড়াতাড়ি ওহী আয়ত্ত করার জন্য আপনি তা নিয়ে আপনার জিহ্বাকে দ্রুত সঞ্চালন করবেন না।"
        }
      ]
    },
    {
      "id": 5568,
      "verse_number": 17,
      "verse_key": "75:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ",
      "text_imlaei": "إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101555,
          "resource_id": 20,
          "text": "Indeed, upon Us is its collection [in your heart] and [to make possible] its recitation."
        },
        {
          "id": 1172164,
          "resource_id": 213,
          "text": "নিশ্চয় এর সংরক্ষণ ও পাঠ করানোর দায়িত্ব আমাদেরই।"
        }
      ]
    },
    {
      "id": 5569,
      "verse_number": 18,
      "verse_key": "75:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا قَرَأْنَـٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ",
      "text_imlaei": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101556,
          "resource_id": 20,
          "text": "So when We have recited it [through Gabriel], then follow its recitation."
        },
        {
          "id": 1172165,
          "resource_id": 213,
          "text": "কাজেই যখন আমরা তা পাঠ করি আপনি সে পাঠের অনুসরণ করুন,"
        }
      ]
    },
    {
      "id": 5570,
      "verse_number": 19,
      "verse_key": "75:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ",
      "text_imlaei": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ",
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 101557,
          "resource_id": 20,
          "text": "Then upon Us is its clarification [to you]."
        },
        {
          "id": 1172166,
          "resource_id": 213,
          "text": "তারপর তার বর্ণনার দায়িত্ব নিশ্চিতভাবে আমাদেরই [১]।"
        }
      ]
    },
    {
      "id": 5571,
      "verse_number": 20,
      "verse_key": "75:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ",
      "text_imlaei": "كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101560,
          "resource_id": 20,
          "text": "No! But you [i.e., mankind] love the immediate"
        },
        {
          "id": 1172167,
          "resource_id": 213,
          "text": "কখনো না, বরং তোমরা দুনিয়ার জীবনকে ভালবাস"
        }
      ]
    },
    {
      "id": 5572,
      "verse_number": 21,
      "verse_key": "75:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَتَذَرُونَ ٱلْـَٔاخِرَةَ",
      "text_imlaei": "وَتَذَرُونَ الْآخِرَةَ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101561,
          "resource_id": 20,
          "text": "And leave [i.e., neglect] the Hereafter."
        },
        {
          "id": 1172168,
          "resource_id": 213,
          "text": "আর তোমরা আখেরাতকে উপেক্ষা কর [১]।"
        }
      ]
    },
    {
      "id": 5573,
      "verse_number": 22,
      "verse_key": "75:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ",
      "text_imlaei": "وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101562,
          "resource_id": 20,
          "text": "[Some] faces, that Day, will be radiant,"
        },
        {
          "id": 1172169,
          "resource_id": 213,
          "text": "সেদিন কোনো কোনো মুখমণ্ডল উজ্জ্বল হবে,"
        }
      ]
    },
    {
      "id": 5574,
      "verse_number": 23,
      "verse_key": "75:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ رَبِّهَا نَاظِرَةٌ",
      "text_imlaei": "إِلَىٰ رَبِّهَا نَاظِرَةٌ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101563,
          "resource_id": 20,
          "text": "Looking at their Lord.<sup foot_note=197660>1</sup>"
        },
        {
          "id": 1172170,
          "resource_id": 213,
          "text": "তারা তাদের রবের দিকে তাকিয়ে থাকবে [১]।"
        }
      ]
    },
    {
      "id": 5575,
      "verse_number": 24,
      "verse_key": "75:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَوُجُوهٌ يَوْمَئِذٍۭ بَاسِرَةٌ",
      "text_imlaei": "وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101564,
          "resource_id": 20,
          "text": "And [some] faces, that Day, will be contorted,"
        },
        {
          "id": 1172171,
          "resource_id": 213,
          "text": "আর কোনো কোনো মুখমণ্ডল হয়ে পড়বে বিবর্ণ,"
        }
      ]
    },
    {
      "id": 5576,
      "verse_number": 25,
      "verse_key": "75:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ",
      "text_imlaei": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101565,
          "resource_id": 20,
          "text": "Expecting that there will be done to them [something] backbreaking."
        },
        {
          "id": 1172172,
          "resource_id": 213,
          "text": "আশংকা করবে যে, এক ধ্বংসকারী বিপর্যয় তাদের উপর আপতিত হবে।"
        }
      ]
    },
    {
      "id": 5577,
      "verse_number": 26,
      "verse_key": "75:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ",
      "text_imlaei": "كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101566,
          "resource_id": 20,
          "text": "No! When it [i.e., the soul] has reached the collar bones<sup foot_note=197661>1</sup>"
        },
        {
          "id": 1172173,
          "resource_id": 213,
          "text": "অবশ্যই [১], যখন প্ৰাণ কণ্ঠাগত হবে"
        }
      ]
    },
    {
      "id": 5578,
      "verse_number": 27,
      "verse_key": "75:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَقِيلَ مَنْ ۜ رَاقٍ",
      "text_imlaei": "وَقِيلَ مَنْ ۜ رَاقٍ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101567,
          "resource_id": 20,
          "text": "And it is said, \"Who will cure [him]?\""
        },
        {
          "id": 1172174,
          "resource_id": 213,
          "text": "এবং বলা হবে, ‘কে তাকে রক্ষা করবে?’"
        }
      ]
    },
    {
      "id": 5579,
      "verse_number": 28,
      "verse_key": "75:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ",
      "text_imlaei": "وَظَنَّ أَنَّهُ الْفِرَاقُ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101568,
          "resource_id": 20,
          "text": "And he [i.e., the dying one] is certain that it is the [time of] separation"
        },
        {
          "id": 1172175,
          "resource_id": 213,
          "text": "তখন তার প্রত্যয় হবে যে, এটা বিদায়ক্ষণ।"
        }
      ]
    },
    {
      "id": 5580,
      "verse_number": 29,
      "verse_key": "75:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
      "text_imlaei": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101569,
          "resource_id": 20,
          "text": "And the leg is wound about the leg,<sup foot_note=197662>1</sup>"
        },
        {
          "id": 1172176,
          "resource_id": 213,
          "text": "আর পায়ের গোছার সঙ্গে পায়ের গোছা জড়িয়ে যাবে [১]।"
        }
      ]
    },
    {
      "id": 5581,
      "verse_number": 30,
      "verse_key": "75:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ",
      "text_imlaei": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101572,
          "resource_id": 20,
          "text": "To your Lord, that Day, will be the procession.<sup foot_note=197663>1</sup>"
        },
        {
          "id": 1172177,
          "resource_id": 213,
          "text": "সেদিন আপনার রবের কাছেই সকলকে হাঁকিয়ে নেয়া হবে।"
        }
      ]
    },
    {
      "id": 5582,
      "verse_number": 31,
      "verse_key": "75:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "text_imlaei": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101573,
          "resource_id": 20,
          "text": "And he [i.e., the disbeliever] had not believed, nor had he prayed."
        },
        {
          "id": 1172178,
          "resource_id": 213,
          "text": "সুতরাং সে বিশ্বাস করে নি এবং সালাতও আদায় করে নি।"
        }
      ]
    },
    {
      "id": 5583,
      "verse_number": 32,
      "verse_key": "75:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَـٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "text_imlaei": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101574,
          "resource_id": 20,
          "text": "But [instead], he denied and turned away."
        },
        {
          "id": 1172179,
          "resource_id": 213,
          "text": "বরং সে মিথ্যারোপ করেছিল এবং মুখ ফিরিয়ে নিয়েছিল।"
        }
      ]
    },
    {
      "id": 5584,
      "verse_number": 33,
      "verse_key": "75:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ",
      "text_imlaei": "ثُمَّ ذَهَبَ إِلَىٰ أَهْلِهِ يَتَمَطَّىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101575,
          "resource_id": 20,
          "text": "And then he went to his people, swaggering [in pride]."
        },
        {
          "id": 1172180,
          "resource_id": 213,
          "text": "তারপর সে তার পরিবার পরিজনের কাছে চলে গিয়েছিল অহংকার করে,"
        }
      ]
    },
    {
      "id": 5585,
      "verse_number": 34,
      "verse_key": "75:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
      "text_imlaei": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101576,
          "resource_id": 20,
          "text": "Woe to you, and woe!"
        },
        {
          "id": 1172181,
          "resource_id": 213,
          "text": "দুর্ভোগ তোমার জন্য, দুর্ভোগ !"
        }
      ]
    },
    {
      "id": 5586,
      "verse_number": 35,
      "verse_key": "75:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ",
      "text_imlaei": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101577,
          "resource_id": 20,
          "text": "Then woe to you, and woe!"
        },
        {
          "id": 1172182,
          "resource_id": 213,
          "text": "আবার দুর্ভোগ তোমার জন্য, দুর্ভোগ [১] !"
        }
      ]
    },
    {
      "id": 5587,
      "verse_number": 36,
      "verse_key": "75:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَيَحْسَبُ ٱلْإِنسَـٰنُ أَن يُتْرَكَ سُدًى",
      "text_imlaei": "أَيَحْسَبُ الْإِنسَانُ أَن يُتْرَكَ سُدًى",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101578,
          "resource_id": 20,
          "text": "Does man think that he will be left neglected?<sup foot_note=197664>1</sup>"
        },
        {
          "id": 1172183,
          "resource_id": 213,
          "text": "মানুষ কি মনে করে যে, তাকে এমনি ছেড়ে দেয়া হবে [১] ?"
        }
      ]
    },
    {
      "id": 5588,
      "verse_number": 37,
      "verse_key": "75:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ يَكُ نُطْفَةً مِّن مَّنِىٍّ يُمْنَىٰ",
      "text_imlaei": "أَلَمْ يَكُ نُطْفَةً مِّن مَّنِيٍّ يُمْنَىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101579,
          "resource_id": 20,
          "text": "Had he not been a sperm from semen emitted?"
        },
        {
          "id": 1172184,
          "resource_id": 213,
          "text": "সে কি বীর্যের স্থলিত শুক্রবিন্দু ছিল না?"
        }
      ]
    },
    {
      "id": 5589,
      "verse_number": 38,
      "verse_key": "75:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ",
      "text_imlaei": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101580,
          "resource_id": 20,
          "text": "Then he was a clinging clot, and [Allāh] created [his form] and proportioned [him]"
        },
        {
          "id": 1172185,
          "resource_id": 213,
          "text": "তারপর সে ‘আলাকা’য় পরিণত হয়। অতঃপর আল্লাহ্ তাকে সৃষ্টি করেন এবং সুঠাম করেন।"
        }
      ]
    },
    {
      "id": 5590,
      "verse_number": 39,
      "verse_key": "75:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
      "text_imlaei": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنثَىٰ",
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 101581,
          "resource_id": 20,
          "text": "And made of him two mates, the male and the female."
        },
        {
          "id": 1172186,
          "resource_id": 213,
          "text": "অতঃপর তিনি তা থেকে সৃষ্টি করেন যুগল--- নর ও নারী।"
        }
      ]
    },
    {
      "id": 5591,
      "verse_number": 40,
      "verse_key": "75:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَيْسَ ذَٰلِكَ بِقَـٰدِرٍ عَلَىٰٓ أَن يُحْـِۧىَ ٱلْمَوْتَىٰ",
