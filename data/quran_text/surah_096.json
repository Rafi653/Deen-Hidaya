  "surah_info": {
    "id": 96,
    "revelation_place": "makkah",
    "revelation_order": 1,
    "bismillah_pre": true,
    "name_simple": "Al-'Alaq",
    "name_complex": "Al-`Alaq",
    "name_arabic": "العلق",
    "verses_count": 19,
    "pages": [
      597,
      597
    ],
    "translated_name": {
      "language_name": "english",
      "name": "The Clot"
    }
  },
  "verses": [
    {
      "id": 6107,
      "verse_number": 1,
      "verse_key": "96:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ",
      "text_imlaei": "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102315,
          "resource_id": 20,
          "text": "Recite in the name of your Lord who created"
        },
        {
          "id": 1172703,
          "resource_id": 213,
          "text": "পড়ুন আপনার রবের নামে, যিনি সৃষ্টি করেছেন [১] –"
        }
      ]
    },
    {
      "id": 6108,
      "verse_number": 2,
      "verse_key": "96:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "خَلَقَ ٱلْإِنسَـٰنَ مِنْ عَلَقٍ",
      "text_imlaei": "خَلَقَ الْإِنسَانَ مِنْ عَلَقٍ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102327,
          "resource_id": 20,
          "text": "Created man from a clinging substance."
        },
        {
          "id": 1172704,
          "resource_id": 213,
          "text": "সৃষ্টি করেছেন মানুষকে ‘আলাক’ হতে [১]।"
        }
      ]
    },
    {
      "id": 6109,
      "verse_number": 3,
      "verse_key": "96:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
      "text_imlaei": "اقْرَأْ وَرَبُّكَ الْأَكْرَمُ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102329,
          "resource_id": 20,
          "text": "Recite, and your Lord is the most Generous -"
        },
        {
          "id": 1172705,
          "resource_id": 213,
          "text": "পড়ুন, আর আপনার রব মহামহিমান্বিত [১]"
        }
      ]
    },
    {
      "id": 6110,
      "verse_number": 4,
      "verse_key": "96:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
      "text_imlaei": "الَّذِي عَلَّمَ بِالْقَلَمِ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102331,
          "resource_id": 20,
          "text": "Who taught by the pen"
        },
        {
          "id": 1172706,
          "resource_id": 213,
          "text": "যিনি কলমের সাহায্যে শিক্ষা দিয়েছেন [১] –"
        }
      ]
    },
    {
      "id": 6111,
      "verse_number": 5,
      "verse_key": "96:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلَّمَ ٱلْإِنسَـٰنَ مَا لَمْ يَعْلَمْ",
      "text_imlaei": "عَلَّمَ الْإِنسَانَ مَا لَمْ يَعْلَمْ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102333,
          "resource_id": 20,
          "text": "Taught man that which he knew not."
        },
        {
          "id": 1172707,
          "resource_id": 213,
          "text": "শিক্ষা দিয়েছেন মানুষকে যা সে জানত না [১]।"
        }
      ]
    },
    {
      "id": 6112,
      "verse_number": 6,
      "verse_key": "96:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّ ٱلْإِنسَـٰنَ لَيَطْغَىٰٓ",
      "text_imlaei": "كَلَّا إِنَّ الْإِنسَانَ لَيَطْغَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102335,
          "resource_id": 20,
          "text": "No! [But] indeed, man transgresses"
        },
        {
          "id": 1172708,
          "resource_id": 213,
          "text": "বাস্তবেই [১], মানুষ সীমালঙ্ঘনই করে থাকে,"
        }
      ]
    },
    {
      "id": 6113,
      "verse_number": 7,
      "verse_key": "96:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
      "text_imlaei": "أَن رَّآهُ اسْتَغْنَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102337,
          "resource_id": 20,
          "text": "Because he sees himself self-sufficient."
        },
        {
          "id": 1172709,
          "resource_id": 213,
          "text": "কারণ সে নিজকে অমুখাপেক্ষী মনে করে [১]।"
        }
      ]
    },
    {
      "id": 6114,
      "verse_number": 8,
      "verse_key": "96:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
      "text_imlaei": "إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102339,
          "resource_id": 20,
          "text": "Indeed, to your Lord is the return."
        },
        {
          "id": 1172710,
          "resource_id": 213,
          "text": "নিশ্চয় আপনার রবের কাছেই ফিরে যাওয়া [১]।"
        }
      ]
    },
    {
      "id": 6115,
      "verse_number": 9,
      "verse_key": "96:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
      "text_imlaei": "أَرَأَيْتَ الَّذِي يَنْهَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102341,
          "resource_id": 20,
          "text": "Have you seen the one who forbids"
        },
        {
          "id": 1172711,
          "resource_id": 213,
          "text": "আমাকে জানাও (এবং আশ্চর্য হও) তার সম্পর্কে, যে বাধা দেয়"
        }
      ]
    },
    {
      "id": 6116,
      "verse_number": 10,
      "verse_key": "96:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَبْدًا إِذَا صَلَّىٰٓ",
      "text_imlaei": "عَبْدًا إِذَا صَلَّىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102316,
          "resource_id": 20,
          "text": "A servant when he prays?"
        },
        {
          "id": 1172712,
          "resource_id": 213,
          "text": "এক বান্দাকে [১]- যখন তিনি সালাত আদায় করেন।"
        }
      ]
    },
    {
      "id": 6117,
      "verse_number": 11,
      "verse_key": "96:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
      "text_imlaei": "أَرَأَيْتَ إِن كَانَ عَلَى الْهُدَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102317,
          "resource_id": 20,
          "text": "Have you seen if he is upon guidance"
        },
        {
          "id": 1172713,
          "resource_id": 213,
          "text": "আমাকে বল! যদি তিনি হিদায়াতের উপর থাকেন"
        }
      ]
    },
    {
      "id": 6118,
      "verse_number": 12,
      "verse_key": "96:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
      "text_imlaei": "أَوْ أَمَرَ بِالتَّقْوَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102318,
          "resource_id": 20,
          "text": "Or enjoins righteousness?"
        },
        {
          "id": 1172714,
          "resource_id": 213,
          "text": "অথবা তাক্ওয়ার নির্দেশ দেন; (তারপরও সে কিভাবে বাধা দেয়?!)"
        }
      ]
    },
    {
      "id": 6119,
      "verse_number": 13,
      "verse_key": "96:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "text_imlaei": "أَرَأَيْتَ إِن كَذَّبَ وَتَوَلَّىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102319,
          "resource_id": 20,
          "text": "Have you seen if he denies and turns away -"
        },
        {
          "id": 1172715,
          "resource_id": 213,
          "text": "আমাকে বল! যদি সে (নিষেধকারী) মিথ্যারোপ করে এবং মুখ ফিরিয়ে নেয়,"
        }
      ]
    },
    {
      "id": 6120,
      "verse_number": 14,
      "verse_key": "96:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "text_imlaei": "أَلَمْ يَعْلَم بِأَنَّ اللَّهَ يَرَىٰ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102320,
          "resource_id": 20,
          "text": "Does he not know that Allāh sees?"
        },
        {
          "id": 1172716,
          "resource_id": 213,
          "text": "সে কি জানে না যে, নিশ্চয় আল্লাহ্ দেখেন [১] ?!"
        }
      ]
    },
    {
      "id": 6121,
      "verse_number": 15,
      "verse_key": "96:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
      "text_imlaei": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102321,
          "resource_id": 20,
          "text": "No! If he does not desist, We will surely drag him by the forelock<sup foot_note=197786>1</sup>"
        },
        {
          "id": 1172717,
          "resource_id": 213,
          "text": "কখনো নয়, সে যদি বিরত না হয় তবে আমরা তাকে অবশ্যই হেঁচড়ে নিয়ে যাব, মাথার সামনের চুলের গুচ্ছ ধরে [১]-----"
        }
      ]
    },
    {
      "id": 6122,
      "verse_number": 16,
      "verse_key": "96:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "نَاصِيَةٍ كَـٰذِبَةٍ خَاطِئَةٍ",
      "text_imlaei": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102322,
          "resource_id": 20,
          "text": "A lying, sinning forelock."
        },
        {
          "id": 1172718,
          "resource_id": 213,
          "text": "মিথ্যাচারী, পাপিষ্ঠ সম্মুখ-চুলের-গুচ্ছ।"
        }
      ]
    },
    {
      "id": 6123,
      "verse_number": 17,
      "verse_key": "96:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلْيَدْعُ نَادِيَهُۥ",
      "text_imlaei": "فَلْيَدْعُ نَادِيَهُ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102323,
          "resource_id": 20,
          "text": "Then let him call his associates;"
        },
        {
          "id": 1172719,
          "resource_id": 213,
          "text": "অতএব সে তার পারিষদকে ডেকে আনুক !"
        }
      ]
    },
    {
      "id": 6124,
      "verse_number": 18,
      "verse_key": "96:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَنَدْعُ ٱلزَّبَانِيَةَ",
      "text_imlaei": "سَنَدْعُ الزَّبَانِيَةَ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102324,
          "resource_id": 20,
          "text": "We will call the angels of Hell.<sup foot_note=197787>1</sup>"
        },
        {
          "id": 1172720,
          "resource_id": 213,
          "text": "শীঘ্রই আমরা ডেকে আনব যাবানিয়াদেরকে [১]।"
        }
      ]
    },
    {
      "id": 6125,
      "verse_number": 19,
      "verse_key": "96:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": 14,
      "text_uthmani": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
      "text_imlaei": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِب",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 102325,
          "resource_id": 20,
          "text": "No! Do not obey him. But prostrate and draw near [to Allāh]."
        },
        {
          "id": 1172721,
          "resource_id": 213,
          "text": "কখনো নয়! আপনি তার অনুসরণ করবেন না। আর আপনি সাজ্দা করুন এবং নিকটবর্তী হোন [১]।"
        }
      ]
    }
  ],
